   "source": [
    "import cv2\n",
    "\n",
    "# Resmi yükleyin\n",
    "img = cv2.imread(\"resimler/2.png\")\n",
    "\n",
    "# Resmin alfa kanalını kaldırın\n",
    "img_bgr = cv2.cvtColor(img, cv2.COLOR_RGBA2BGR)\n",
    "\n",
    "# Resmi kaydedin\n",
    "cv2.imwrite(\"resimler/2.png\", img_bgr)"
   ]
